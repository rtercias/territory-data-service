    "apollo-server-express": "^1.4.0",
    "apollo-tracing": "^0.9.1",
    "cluster": "^0.7.7",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "express": "^4.17.1",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.1.1",
    "mysql": "^2.18.1",
    "os": "^0.1.1",
