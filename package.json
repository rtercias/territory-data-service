{
  "name": "territory-data-service",
  "version": "1.0.0",
  "description": "graphQL service",
  "main": "dist/functions.js",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "lint": "xo",
    "config": "firebase functions:config:get > .runtimeconfig.json",
    "watch": "parcel src/functions.js --target node",
    "build": "parcel build src/functions.js --target node --detailed-report",
    "serve": "export NODE_ENV=development && yarn build && firebase serve",
    "emulator": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "yarn serve",
    "predeploy": "yarn build",
    "predeploy-test": "yarn build",
    "deploy": "firebase deploy --only functions",
    "deploy-test": "firebase deploy --only functions -P foreign-field-test",
    "logs": "firebase functions:log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtercias/territory-data-service.git"
  },
  "author": "Ryan Tercias",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rtercias/territory-data-service/issues"
  },
  "homepage": "https://github.com/rtercias/territory-data-service#readme",
  "xo": {
    "space": true
  },
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "axios": "^0.21.2",
    "bl": "^4.0.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-and-time": "^0.14.2",
    "date-fns": "^2.14.0",
    "elliptic": "^6.5.4",
    "express": "^4.17.1",
    "firebase-admin": "8.13.0",
    "firebase-functions": "3.21.1",
    "firebase-tools": "8.20.0",
    "graphql": "^14.3.0",
    "graphql-tools": "6.2.6",
    "is-svg": "^4.2.2",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "node-forge": "^0.10.0",
    "os": "^0.1.1",
    "promise-mysql": "^3.3.2",
    "pusher": "^3.0.1",
    "twilio": "^3.71.3",
    "util": "^0.11.1",
    "xmldom": "^0.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^9.0.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "chokidar": "^3.4.0",
    "eslint": "^4.19.1",
    "firebase-functions-test": "^0.2.1",
    "parcel-bundler": "^1.12.3",
    "xo": "^0.24.0"
  }
}
